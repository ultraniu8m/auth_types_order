<Window x:Class="_04_06_dist.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_04_06_dist"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinWidth="750" MinHeight="380">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="56*"></RowDefinition>
            <RowDefinition Height="275*"></RowDefinition>
            <RowDefinition Height="56*"></RowDefinition>
        </Grid.RowDefinitions>
        <Rectangle Fill="#FF498C51" HorizontalAlignment ="Stretch"  Stroke="Black" VerticalAlignment="Stretch"/>
        <Rectangle Fill="#FF76E383" HorizontalAlignment="Stretch"  Stroke="Black" VerticalAlignment="Stretch"  Grid.Row="2" MinWidth="600" MinHeight="350"/>
        <Frame x:Name="main_frame"  HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" NavigationUIVisibility="Hidden"/>
        <Image HorizontalAlignment="Left"  VerticalAlignment="Top"  Source="logo.png"/>




    </Grid>
</Window>
